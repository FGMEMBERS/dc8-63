<?xml version="1.0"?>
<PropertyList>

 <path>boeing747-400-ai-jw.ac</path>
 
  <animation>
  <type>scale</type>
  <x-offset>1.35</x-offset>
  <y-offset>1.35</y-offset>
  <z-offset>1.35</z-offset>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>/orientation/roll-deg</property>
  <center>
   <x-m>-0.042</x-m>
   <y-m>0</y-m>
   <z-m>-0.004</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <property>/orientation/pitch-deg</property>
  <center>
   <x-m>-0.042</x-m>
   <y-m>0</y-m>
   <z-m>-0.004</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>rollarrow</object-name>
  <property>/orientation/roll-deg</property>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
  <animation>
   <object-name>ball</object-name>
   <object-name>wings</object-name>
   <object-name>riminside</object-name>
   <object-name>rollindex</object-name>
   <object-name>rollarrow</object-name>
   <object-name>CPH2</object-name>
   <object-name>LPanelUH</object-name>   
   <type>material</type>
     <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
		     <red>0.6</red>
		     <green>0.3</green>
		     <blue>0.3</blue>
      </emission>
  </animation>

</PropertyList>
