<!--
DC-8-63 cockpit
Isaias Prestes - a generic cockpit for DC-8
-->

<PropertyList>
 <path>Aircraft/dc8-63/Models/dc8_cockpit.ac</path>
 
    <animation>
        <type>material</type>
        <object-name>panel_cap_firstofficer</object-name>
        <object-name>central</object-name>
        <object-name>piano</object-name>
        <object-name>windows_structure</object-name>
        <object-name>structpainel</object-name>
        <emission>
            <red>0.8</red>
            <green>0.4</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

 
    <animation>
        <type>select</type>
        <object-name>instr1</object-name>
        <condition>
        	<not>
            <property>sim/current-view/internal</property>
            </not>
        </condition>
    </animation>

